{
	"name": "image_resizer",
	"productName": "Image_Resizer",
	"version": "1.0.0",
	"description": "A little app to resize images",
	"main": "main.js",
	"scripts": {
		"app": "electron .",
		"dev": "nodemon --exec electron .",
		"package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
		"package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=false --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Image_Resizer\"",
		"package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=false --out=release-builds"
	},
	"author": "Tim Lui",
	"license": "MIT",
	"devDependencies": {
		"electron": "^11.5.0",
		"electron-packager": "^15.0.0",
		"nodemon": "^2.0.4"
	},
	"dependencies": {
		"electron-log": "^4.2.2",
		"imagemin": "^7.0.1",
		"imagemin-mozjpeg": "^9.0.0",
		"imagemin-pngquant": "^9.0.0",
		"slash": "^3.0.0"
	}
}
